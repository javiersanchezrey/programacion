package Ejercicios;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

public class Num9 {

	public static void main(String[] args) throws IOException {
		
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));

		
		System.out.println("¿Cuál es tu Edad?");
		int edad1 = Integer.parseInt(br.readLine());
		
	System.out.println("¿Cuál es tu altura?");
		double altura11 = Double.parseDouble(br.readLine());
		
	System.out.println("¿Cuál es tu peso?");
		double peso1 = Double.parseDouble(br.readLine());
		
	double BWT = 2.447 - 0.09156 * edad1 + 0.1074 * altura11 + 0.3362 * peso1;
	
	System.out.println("El BWT del paciente con " + edad1 + " años y altura de " + altura11 +" metros es de " + BWT);
					
			
			
			
			
			
			
		
		

	}

}
